{
    "name": "eslint-config-get-off-my-lawn",
    "description": "A highly opinionated, sharable config of ESLint rules to produce beautiful, readable JavaScript.",
    "version": "5.0.0-beta.1",
    "author": {
        "name": "Michael Novotny",
        "email": "manovotny@gmail.com",
        "url": "https://manovotny.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/manovotny/eslint-config-get-off-my-lawn.git"
    },
    "keywords": [
        "babel",
        "config",
        "eslint",
        "react"
    ],
    "engines": {
        "node": ">=8.6.0",
        "yarn": ">=1.0.0"
    },
    "main": "index.js",
    "files": [
        "src"
    ],
    "scripts": {
        "prettier": "prettier --write \"**/*.{js,json,md,yml}\"",
        "test": "eslint . --ext .js --ext .json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,md,yml}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "babel-eslint": "10.0.1",
        "dot-prop": "5.0.0",
        "eslint-config-prettier": "4.3.0",
        "eslint-plugin-eslint-comments": "3.1.1",
        "eslint-plugin-get-off-my-lawn": "1.0.2",
        "eslint-plugin-import": "2.17.2",
        "eslint-plugin-jest": "22.5.1",
        "eslint-plugin-json": "1.4.0",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-node": "9.0.1",
        "eslint-plugin-objects": "1.1.1",
        "eslint-plugin-react": "7.13.0",
        "eslint-plugin-react-hooks": "1.6.0",
        "eslint-plugin-react-native": "3.7.0",
        "eslint-plugin-security": "1.4.0",
        "eslint-plugin-unicorn": "8.0.2",
        "read-pkg-up": "6.0.0",
        "semver": "6.0.0"
    },
    "devDependencies": {
        "eslint": "5.16.0",
        "husky": "2.3.0",
        "lint-staged": "8.1.7",
        "prettier": "1.17.1"
    },
    "peerDependencies": {
        "eslint": ">=5.16.0"
    }
}
